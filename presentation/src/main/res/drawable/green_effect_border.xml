<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#4e8b58" />
            <stroke
                android:width="1dp"
                android:color="#334b3a" />
            <corners
                android:radius="8dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#76f585"
                android:endColor="#234b26"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#334b3a" />
            <corners
                android:radius="3dp" />
        </shape>
    </item>
</selector>